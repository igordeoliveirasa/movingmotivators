<h1>Listing user_game_level_motivators</h1>

<table>
  <thead>
    <tr>
      <th>User game level</th>
      <th>Motivator</th>
      <th>X</th>
      <th>Y</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_game_level_motivators.each do |user_game_level_motivator| %>
      <tr>
        <td><%= user_game_level_motivator.user_game_level %></td>
        <td><%= user_game_level_motivator.motivator %></td>
        <td><%= user_game_level_motivator.x %></td>
        <td><%= user_game_level_motivator.y %></td>
        <td><%= link_to 'Show', user_game_level_motivator %></td>
        <td><%= link_to 'Edit', edit_user_game_level_motivator_path(user_game_level_motivator) %></td>
        <td><%= link_to 'Destroy', user_game_level_motivator, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User game level motivator', new_user_game_level_motivator_path %>
